#pragma once

#include "font.h"

extern const std::uint8_t _binary_@font_name@_bin_start[];
extern const std::uint8_t _binary_@font_name@_bin_end[];

inline Font @font_name@() {
  auto data = std::span(_binary_@font_name@_bin_start,
                        _binary_@font_name@_bin_end);
  return Font{.width = data[0],
              .height = data[1],
              .data = data.subspan(2)};
}
